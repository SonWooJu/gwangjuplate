<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smart.project.proc.Test" >
    <select id="sqlMenu2" resultType="com.smart.project.web.home.vo.TestVO" parameterType="java.lang.String">
        SELECT * FROM test;
    </select>
    <insert id="insertjson" parameterType="java.util.List" >
    insert into res_tb(num,tel,postcode,locaddr,roadaddr,road_post,workplace,state,coorx,coory) values
    <foreach collection="list" item="item" separator=",">
        (#{item.num},#{item.tel},#{item.postcode},#{locaddr},#{item.roadaddr},#{item.road_post},
         #{item.workplce},#{item.state},#{item.coorx},#{item.coory})


    </foreach>
    </insert>
    <insert id="joinInsert" parameterType="com.smart.project.web.home.vo.MemberVO">
        insert into userRegister (userId, userPw, userPwchk, userName, userDate, userSex, userPhnum, userPolicy, userAuth, appendDate, updateDate)
        values(#{userId},#{userPw},#{userPwchk},#{userName},#{userDate},#{userSex},#{userPhnum},#{userPolicy}, #{userAuth}, #{appendDate}, #{updateDate});

    </insert>
    <insert id="kakaoInsert">
        INSERT INTO kakao_table (USERID, USERPW, USERNAME, EMAIL, SNSID) VALUES
            (#{userid}, #{userpw}, #{userName}, #{email}, #{snsId})
    </insert>
    <select id="getUserAccount" resultType="com.smart.project.web.home.vo.MemberVO">
        select * FROM userRegister WHERE userId = #{userId}
    </select>
</mapper>